{
  "name": "jumbosmash-server",
  "version": "2.1.1",
  "description": "JumboSmash 2019",
  "engines": {
    "node": "^8.15.0",
    "postgres": "^10.5.x"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "export NODE_ENV=development;npm run check-env --silent && nodemon --exec babel-node server.js",
    "dev:nocheck": "NODE_ENV=development babel-node server.js",
    "staging": "export NODE_ENV=staging;npm run check-env --silent && node server.js",
    "prod": "export NODE_ENV=production;npm run check-env --silent && node server.js",
    "migrate": "node-pg-migrate -m ./db/migrations",
    "build": "rm -rf ./dist && babel . --ignore node_modules,dist -d dist && cp .npmrc dist && cp -r config dist && cp *.json dist",
    "test": "npm run migrate up && npm run jest",
    "jest": "jest --runInBand --forceExit",
    "check-env": "babel-node ./scripts/check-environment",
    "check-node-version": "node ./scripts/check-node-version.js",
    "check-dependencies": "node ./scripts/check-dependencies.js",
    "check-database": "node ./scripts/check-database.js",
    "drop-all-tables": "babel-node ./scripts/drop-all-tables.js",
    "reset-db": "npm run drop-all-tables && npm run migrate up",
    "lint": "eslint .",
    "lint:watch": "nodemon --exec eslint -- .",
    "release": "release-it",
    "bump-deploy-version": "babel-node ./scripts/bump-deploy-versions.js",
    "insert-admin": "node ./scripts/insert-admin.js"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^0.11.0",
    "@sendgrid/mail": "^6.3.1",
    "@sentry/node": "^5.0.8",
    "@slack/client": "^4.8.0",
    "ajv": "^6.5.4",
    "aws-sdk": "^2.382.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "bull": "^3.7.0",
    "chance": "^1.0.18",
    "config": "^2.0.1",
    "expo-server-sdk": "^3.1.0",
    "express": "^4.16.4",
    "express-mung": "^0.5.1",
    "ioredis": "^4.9.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "node-pg-migrate": "^3.18.1",
    "pg": "^7.6.0",
    "request": "^2.88.0",
    "semver": "^5.6.0",
    "socket.io": "^2.2.0",
    "socket.io-redis": "^5.2.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.0",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.5",
    "regenerator-runtime": "^0.12.1",
    "release-it": "^10.2.0",
    "supertest": "^3.3.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./scripts/jest-setup.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/config/"
    ]
  }
}
