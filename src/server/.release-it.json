{
  "preReleaseId": "beta",
  "git": {
    "tagName": "server-${version}",
    "tagAnnotation": "Server ${version} release",
    "commitMessage": "Release server ${version}"
  },
  "github": {
    "release": true,
    "releaseName": "Server - ${version}"
  },
  "npm": {
    "publish": false
  },
  "scripts": {
    "beforeBump": "npm run bump-deploy-version ${version} && git add --all && git commit -m \"Bump deploy files to ${version}\""
  }
}
